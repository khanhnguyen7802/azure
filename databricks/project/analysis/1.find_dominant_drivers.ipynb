   "source": [
    "SELECT\n",
    "  COUNT(1) AS total_races, \n",
    "  driver_name, \n",
    "  SUM(calculated_points) AS total_points,\n",
    "  AVG(calculated_points) AS avg_points\n",
    "FROM f1_presentation.calculated_race_results \n",
    "GROUP BY driver_name\n",
    "HAVING COUNT(1) >= 50\n",
    "ORDER BY total_points DESC"
   ]
